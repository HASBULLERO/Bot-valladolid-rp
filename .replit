# Ensure this is at the top of your `.replit` file, outside of any `[`-bracketed section
audio = true

# Specify the Node.js version you want
modules = ["nodejs-20"]

# Set the main entry point for your application
entrypoint = "index.js"

# Define the command to run your application
[run]
args = ["node", "index.js"]

[[ports]]
localPort = 3000
externalPort = 80
